private void SumProductMinMax(int number1, int number2, int number3)
    {
        int  sum = 0;
        int  product = 1;
	int  min=number1;
        int  max=number1;

        sum=number1 + number2 +number3;
        System.out.println("The sum of the numbers is " + sum);

        product= =number1 * number2 *  number3;
  	System.out.println("The product of the numbers is " + product);

	if(number2<number1){
	min=number2;
	}
	if(number3<number2){
	min=number3;
	}
	System.out.println("The min of the umbers is " + min);

	if(number2>number1){
	max=number2;
	}
	if(number3>number2){
	max=number3;
	}
	System.out.println("The max of the umbers is " + max);
    }