 private void UniqueCaracters(String str)  
    {
    

	for (int j = 0; j <= str.length(); j++) {
            for (int j = I+1; j <= str.length(); j++) {
            	if (str.charAt(i)  == str.charAt(j)) {
                	return false;
          	 }
           }
	return true;
	}
    
}